/* cabin-regular - latin */
@font-face {
    font-family: 'Cabin';
    font-style: normal;
    font-weight: 400;
    font-display: swap;
    src: url('../assets/fonts/cabin/cabin-v14-latin-regular.eot'); /* IE9 Compat Modes */
    src: local('Cabin'), local('Cabin-Regular'),
         url('../assets/fonts/cabin/cabin-v14-latin-regular.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
         url('../assets/fonts/cabin/cabin-v14-latin-regular.woff2') format('woff2'), /* Super Modern Browsers */
         url('../assets/fonts/cabin/cabin-v14-latin-regular.woff') format('woff'), /* Modern Browsers */
         url('../assets/fonts/cabin/cabin-v14-latin-regular.ttf') format('ttf'), /* Safari, Android, iOS */
         url('../assets/fonts/cabin/cabin-v14-latin-regular.svg#Cabin') format('svg'); /* Legacy iOS */
  }
  /* cabin-italic - latin */
  @font-face {
    font-family: 'Cabin';
    font-style: italic;
    font-weight: 400;
    font-display: swap;
    src: url('../assets/fonts/cabin/cabin-v14-latin-italic.eot'); /* IE9 Compat Modes */
    src: local('Cabin Italic'), local('Cabin-Italic'),
         url('../assets/fonts/cabin/cabin-v14-latin-italic.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
         url('../assets/fonts/cabin/cabin-v14-latin-italic.woff2') format('woff2'), /* Super Modern Browsers */
         url('../assets/fonts/cabin/cabin-v14-latin-italic.woff') format('woff'), /* Modern Browsers */
         url('../assets/fonts/cabin/cabin-v14-latin-italic.ttf') format('ttf'), /* Safari, Android, iOS */
         url('../assets/fonts/cabin/cabin-v14-latin-italic.svg#Cabin') format('svg'); /* Legacy iOS */
  }
  /* cabin-700 - latin */
  @font-face {
    font-family: 'Cabin';
    font-style: normal;
    font-weight: 700;
    font-display: swap;
    src: url('../assets/fonts/cabin/cabin-v14-latin-700.eot'); /* IE9 Compat Modes */
    src: local('Cabin Bold'), local('Cabin-Bold'),
         url('../assets/fonts/cabin/cabin-v14-latin-700.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
         url('../assets/fonts/cabin/cabin-v14-latin-700.woff2') format('woff2'), /* Super Modern Browsers */
         url('../assets/fonts/cabin/cabin-v14-latin-700.woff') format('woff'), /* Modern Browsers */
         url('../assets/fonts/cabin/cabin-v14-latin-700.ttf') format('ttf'), /* Safari, Android, iOS */
         url('../assets/fonts/cabin/cabin-v14-latin-700.svg#Cabin') format('svg'); /* Legacy iOS */
  }
